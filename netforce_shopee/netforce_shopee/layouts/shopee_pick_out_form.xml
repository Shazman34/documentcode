<inherit inherit="pick_out_form">
    <tab string="Shipping" position="append">
        <separator string="Airway Billing Details"/>
        <field name="recipient_first_name"/>
        <field name="recipient_last_name"/>
        <field name="recipient_address"/>
        <field name="recipient_address2"/>
        <field name="recipient_city"/>
        <field name="recipient_postcode"/>
        <field name="recipient_province"/>
        <field name="recipient_country"/>
        <field name="recipient_phone"/>
        <separator/>
        <field name="shopee_logistics_channel_id"/>
        <field name="shopee_service_code"/>
        <field name="shopee_first_mile_name"/>
        <field name="shopee_last_mile_name"/>
        <field name="shopee_zone"/>
        <field name="shopee_warehouse_address"/>
        <field name="shopee_warehouse_id"/>
        <separator string="Sort Codes"/>
        <field name="shopee_first_recipient_sort_code"/>
        <field name="shopee_second_recipient_sort_code"/>
        <field name="shopee_third_recipient_sort_code"/>
        <field name="shopee_first_sender_sort_code"/>
        <field name="shopee_second_sender_sort_code"/>
        <field name="shopee_third_sender_sort_code"/>
        <field name="shopee_return_first_sort_code"/>
    </tab>
</inherit>
