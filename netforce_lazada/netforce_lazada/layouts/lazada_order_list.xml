<list model="lazada.order" colors='{"#f66":[["show_warning","=",true]]}'>

    <head>
        <!--<button string="Get Shipping Parameter" method="get_shipping_parameter"/>-->
        <!--<button string="Ship Order" method="ship_order"/>-->
        <button string="Get Tracking Number" method="get_tracking_number"/>
        <button string="Get Invoice Detail" method="get_invoice_detail"/>
        <!--<button string="Copy to Sale Order" method="copy_to_sale"/>-->
        <!--<button string="Copy to Goods Issue" method="copy_to_picking"/>-->
        <button string="Copy to Goods Issue" method="copy_to_picking_skip_error" show_progress="1"/>
        <button string="Copy to Customer Invoice" method="copy_to_invoice" show_progress="1"/>
        <button string="Match Invoice" method="match_invoice"/>
        <button string="Match Goods Issue" method="match_picking"/>
<!--        <button string="Refresh Orders" method="refresh_order" show_progress="1"/>-->
<!--        <button string="Refresh Warnings" method="function_store"/>-->
        <button string="Order Repack" method="repack_order"/>
        <button string="Order Cancel" method="cancel_order"/>
        <button string="Ready to ship" method="ready_to_ship"/>
        <button string="Order Delivered" method="order_delivered"/>

    </head>
    <field name="account_id"/>
    <field name="order_sn"/>
    <field name="region"/>
    <field name="total_amount"/>
    <field name="created_at"/>
    <field name="updated_at"/>
    <field name="qty"/>
    <field name="tracking_number"/>
    <field name="order_status"/>
    <field name="show_warning" invisible="1"/>
</list>
